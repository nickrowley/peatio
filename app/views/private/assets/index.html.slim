    - if current_user
      div data-scroll-index=0 = render 'liability_proof'

    div data-scroll-index=1 = render 'btc_assets'
    div data-scroll-index=2 = render 'cny_assets'
    div data-scroll-index=3 = render 'bcr_assets'
    div data-scroll-index=4 = render 'ltc_assets'
    div data-scroll-index=5 = render 'dash_assets'
    div data-scroll-index=6 = render 'usd_assets'
