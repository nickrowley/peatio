      li        data-scroll-nav='1'
        a = t('.btc-assets')
      li        data-scroll-nav='2'
        a = t('.cny-assets')
      li        data-scroll-nav='3'
        a = t('.bcr-assets')
      li        data-scroll-nav='4'
        a = t('.ltc-assets')        
      li        data-scroll-nav='5'
        a = t('.dash-assets')
      li        data-scroll-nav='6'
        a = t('.usd-assets')
      li        data-scroll-nav='7'
        a = t('.gbp-assets')
      li        data-scroll-nav='8'
        a = t('.eur-assets')
      li        data-scroll-nav='9'
        a = t('.zar-assets')
      li        data-scroll-nav='10'
